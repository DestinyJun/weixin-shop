{
  "name": "weixin-shop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-http-dev": "ng serve --open --host 0.0.0.0 --proxy-config proxy.config.json",
    "start-http-prod": "ng serve --open --prod --host 0.0.0.0",
    "start-https-dev": "ng serve --open --host 0.0.0.0 --ssl true --ssl-key \"ssl/server.key\" --ssl-cert \"ssl/server.crt\"",
    "start-https-prod": "ng serve --open --prod --host 0.0.0.0 --ssl true --ssl-key \"ssl/server.key\" --ssl-cert \"ssl/server.crt\"",
    "build": "ng build --prod --base-href /moyaoView/",
    "build-dev": "ng build --base-href /moyaoView/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@cmgustavo/ngx-qrcode": "^1.0.1",
    "angularx-qrcode": "^1.5.3",
    "core-js": "^2.5.4",
    "echarts": "^4.1.0",
    "font-awesome": "^4.7.0",
    "ngx-echarts": "^4.1.0",
    "ngx-loading": "^3.0.1",
    "ngx-weui": "^7.0.0-rc.1",
    "qrcode.js": "0.0.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/echarts": "^4.1.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "g2-mobile": "^2.2.0-beta.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
